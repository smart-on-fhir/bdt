{
  "name": "bdt",
  "version": "2.0.0",
  "description": "Test Suite and Test Runner for Bulk Data FHIR servers",
  "types": "./build/types/index.d.ts",
  "main": "./build/index",
  "bin": {
    "bdt": "./build/lib/bin.js"
  },
  "engines": {
    "node": ">=14"
  },
  "scripts": {
    "test": "lab -v --sourcemaps --transform node_modules/lab-transform-typescript ./test/*.test.ts",
    "build": "tsc --build tsconfig.json",
    "clean": "rm -rf build/*",
    "build:watch": "tsc --build tsconfig.json --incremental --watch",
    "doc": "typedoc --watch && touch docs/api/.nojekyll"
  },
  "keywords": [
    "test",
    "Bulk Data",
    "FHIR"
  ],
  "author": "Vladimir Ignatov <vlad.ignatov@gmail.com>",
  "license": "ISC",
  "dependencies": {
    "@hapi/code": "^8.0.3",
    "code": "^5.2.4",
    "colors": "^1.4.0",
    "commander": "^7.2.0",
    "glob": "^7.1.6",
    "got": "^11.8.5",
    "is-base64": "^1.1.0",
    "jsonwebtoken": "^8.5.1",
    "jwk-lite": "^0.1.3",
    "jwk-to-pem": "^2.0.4",
    "markdown-it": "^12.3.2",
    "moment": "^2.27.0",
    "ms": "^2.1.2",
    "node-jose": "^2.0.0",
    "open": "^8.4.0",
    "pug": "^3.0.2",
    "request": "^2.88.2",
    "slug": "^5.0.1",
    "typescript": "^4.2.4",
    "validator": "^13.6.0"
  },
  "devDependencies": {
    "@hapi/lab": "^24.2.1",
    "@types/glob": "^7.1.3",
    "@types/is-base64": "^1.1.0",
    "@types/jsonwebtoken": "^8.5.1",
    "@types/jwk-to-pem": "^2.0.0",
    "@types/markdown-it": "^12.0.1",
    "@types/ms": "^0.7.31",
    "@types/node": "^18.0.0",
    "@types/node-jose": "^1.1.6",
    "@types/pug": "^2.0.4",
    "@types/request": "^2.48.5",
    "@types/slug": "^0.9.1",
    "@types/validator": "^13.1.3",
    "lab-transform-typescript": "^3.0.1",
    "typedoc": "^0.20.36"
  }
}
